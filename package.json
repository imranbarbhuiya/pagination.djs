{
  "name": "pagination.djs",
  "version": "3.5.1",
  "description": "A discord.js compatible pagination module",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "docs": "typedoc",
    "build": "tsup && tsc -b src",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "rollup-type-bundler"
  },
  "keywords": [
    "pagination",
    "discord",
    "discord.js",
    "pagination.djs",
    "pagination-discord",
    "pagination-discord.js",
    "pagination-djs",
    "discord-pagination",
    "discord-button-pagination"
  ],
  "author": "imranbarbhuiya",
  "license": "MIT",
  "files": [
    "dist",
    "!dist/*.tsbuildinfo"
  ],
  "devDependencies": {
    "@favware/rollup-type-bundler": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "discord.js": "^13.6.0",
    "eslint": "^8.11.0",
    "tsup": "^5.12.1",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "discord.js": ">=13.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imranbarbhuiya/pagination.djs.git"
  },
  "bugs": {
    "url": "https://github.com/imranbarbhuiya/pagination.djs/issues"
  },
  "homepage": "https://pagination-djs.js.org",
  "packageManager": "yarn@3.2.0"
}
